{% extends 'base.html.twig' %}

{% block title %}Other index{% endblock %}

{% block body %}
    <h1>Other index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Date</th>
                <th>Description</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for other in others %}
            <tr>
                <td>{{ other.id }}</td>
                <td>{{ other.Name }}</td>
                <td>{{ other.Date }}</td>
                <td>{{ other.Description }}</td>
                <td>
                    <a href="{{ path('app_other_show', {'id': other.id}) }}">show</a>
                    <a href="{{ path('app_other_edit', {'id': other.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_other_new') }}">Create new</a>
{% endblock %}
