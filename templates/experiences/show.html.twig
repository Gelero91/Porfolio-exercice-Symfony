{% extends 'base.html.twig' %}

{% block title %}Experiences{% endblock %}

{% block body %}
    <h1>Experiences</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ experience.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ experience.nom }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{{ experience.date }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ experience.description }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_experiences_index') }}">back to list</a>

    <a href="{{ path('app_experiences_edit', {'id': experience.id}) }}">edit</a>

    {{ include('experiences/_delete_form.html.twig') }}
{% endblock %}
